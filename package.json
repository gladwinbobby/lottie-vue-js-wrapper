{
  "private": false,
  "name": "lottie-vue-js-wrapper",
  "version": "1.0.0",
  "description": "A simple wrapper for integrating lottie-web into VueJs",
  "main": "dist/lottie-animation.umd.js",
  "module": "dist/lottie-animation.esm.js",
  "unpkg": "dist/lottie-animation.min.js",
  "browser": {
    "./sfc": "src/lottie-animation.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/distlottie-animation.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/lottie-animation.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/lottie-animation.min.js"
  },
  "dependencies": {
    "lottie-web": "^5.6.2",
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "rollup": "^1.29.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify": "^2.0.3",
    "vue-template-compiler": "^2.6.11",
    "vuelidate": "^0.7.5",
    "vuex-persistedstate": "^2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gladwinbobby/lottie-vue-js-wrapper.git"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.js"
  ],
  "keywords": [
    "VueJs",
    "Vue",
    "Lottie",
    "Lottie-web",
    "Bodymovin",
    "Airbnb",
    "lottie-vue-js",
    "vue-lottie",
    "lottie",
    "vue",
    "vue",
    "vue-component",
    "animation",
    "keyframe",
    "After Effects",
    "svg",
    "svg animation"
  ],
  "author": "Gladwin Henald",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gladwinbobby/lottie-vue-js-wrapper/issues"
  },
  "homepage": "https://github.com/gladwinbobby/lottie-vue-js-wrapper#readme"
}
